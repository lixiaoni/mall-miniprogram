}
/* 搜索 */
.goods-ser{
  height: 80rpx;
  position: relative;
  background: #fff;
}
.goods-ser input{
  color: #888888;
  font-size: 28rpx;
  height: 80rpx;
  padding-left: 90rpx;
}
.home-ser{
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  left: 30rpx;z-index: 1000;
  top: 20rpx;
}
.fw-title{
  color:#333;
  font-size: 28rpx;
  height: 70rpx;
  background: #fff;
  text-align: center;
  padding-top: 10rpx;
}
.fw-title image{
  width: 12rpx;height: 12rpx;vertical-align: middle;
}
.fw-title>view{
  display: inline-block;
  width: 50%;
}
.fw-title>view.active{
  color: #CCAA77;
}
.fw-title>view.active view{
  border-bottom: 2rpx solid #CCAA77;
  width:140rpx;padding-bottom: 25rpx;
  margin:0 auto
}
.init-page{
  padding: 0 30rpx;
  position: relative;
}
.init-page image{
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  top:45rpx;
  right:50rpx;
}
.title{
    font-size: 30rpx;
    margin: 40rpx 0;font-weight:bold;
}
.title text{
    padding-bottom: 10rpx;
    color: #333;
}
.tags view{
  display: inline-block;text-align: center;margin-right: 20rpx;
}
.tags view image{
 position: relative;color: #CCAA77;right: 0;top: 0;vertical-align: middle;margin-right: 10rpx;margin-top: -2rpx;
}
.tags view.img{
color: #CCAA77;background: #F7F7F7;border:2rpx solid #CCAA77;height: 68rpx;line-height: 68rpx;
}
.tags view>view{
    display: inline-block;
    font-size: 30rpx;
    color: #333;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 10rpx;
    text-align: center;min-width: 140rpx;
    background: #F1F1F1;padding:0 10rpx;
    margin-bottom: 20rpx;
    word-break: break-all;
}
.tags text:nth-child(odd){
  margin-left: 4%;
  margin-right: 3%;
}
.keywords{
    position: absolute;
    width: 100%;
    left: 0;
    top: 90rpx;
    background: #fff;
    z-index: 9;
    padding: 20rpx;
    box-sizing: border-box;
}
.keywords-item{
    line-height: 70rpx;
    border-bottom: 1px solid #eee;
    color: #787878;
    font-size: 28rpx;
}
.result{
    margin-top: 30rpx;
    padding-left: 4%;
}
.result-list{
    display: inline-block;
    width: 330rpx;
    overflow: hidden;
    height: 525rpx;
    background: #fff;
    margin:0 20rpx 20rpx 0;
    font-size: 26rpx;
    color:#333;
    border-radius: 8rpx;
    text-align: left;
    border:1rpx solid #F5F5F5;
}
.result-list image{
    width: 330rpx;
    background: #EBEAEA;
    height: 330rpx;
}
.result-list .result-text{
  margin-left: 20rpx;
  margin-right:15rpx;
   text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.result-list .result-text:last-child{
    margin-top:10rpx;
}
.result-list .result-text1,.result-list .result-text1 text.h-font{
  color: #666;
}
.result-list .result-text1 text{
  color: #CCAA77;margin-left: 10rpx;
}
.result-list .result-text1 image{
  width: 30rpx;height: 30rpx;vertical-align: middle;background: none;margin-top: -2rpx;margin-left:15rpx;
}
.null-tips{
    padding: 200rpx 0;
    text-align: center;
    color: #787878;
}
.nomore{
  color: #999;
  font-size: 28rpx;
  margin-bottom: 40rpx;
  margin-top: 15rpx;
  text-align: center;
}
.nomore image{
  width: 150rpx;height: 150rpx;margin-top: 90rpx;
  margin-bottom:40rpx;
}
.erm-cont{
  text-align: center;
}
.erm-cont view{
  color: #999;
  font-size: 30rpx;
}
.erm-cont .erm-img{
  width: 550rpx;margin:0 auto;
  height: 550rpx;padding-top:40rpx;padding-bottom:50rpx;
}
.erm-cont image{
  width: 550rpx;
}
.weui-bottom{
  position: fixed;
  bottom: 0;
  color: #CCAA77;
  font-size: 36rpx;
  background:#fff;
  width: 100%;
  height: 120rpx;
  z-index:9999;
}
.weui-bottom view{
  width: 50%;
  text-align: center;
  line-height: 120rpx;
  display: inline-block;
}
.weui-bottom view:last-child{
  color: #fff;
  background:#CCAA77;
  position: absolute;
  right: 0;
  line-height: 122rpx;
  height: 122rpx;
  top:0rpx;
}
.home-ser-new{
  position: absolute;
  top: 32rpx;
  left: 45rpx;
  height: 40rpx;
  width: 40rpx;
}
/* //购物车和列表 */
