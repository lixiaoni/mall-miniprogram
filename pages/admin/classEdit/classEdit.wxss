@import '../class/class.wxss';
.ed-edit{
  background:#FBFBFB;
  height: 90rpx;
  border-top:1rpx solid #ECECEC;
  border-bottom:1rpx solid #ECECEC;
}
.ed-edit view{
  width:49%;
  display: inline-block;
  text-align: center;
  color: #666;
  font-size: 30rpx;
  height: 40rpx;
  margin-top: 20rpx;
}
.ed-edit view:first-child{
   border-right:1rpx solid #ECECEC;
}
.ed-cont{
  margin-bottom: 120rpx;
  background: #fff;
  border-top:1rpx solid #ECECEC;
}
.weui-img image {
  height: 40rpx;
  width: 40rpx;
}