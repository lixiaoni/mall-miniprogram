@import '../follow/follow';
.fw-store .button{
  color: #999;font-size:24rpx;
  height: 35rpx;background: #F5F5F5;border-radius: 10rpx;margin-top: 20rpx;
  display: inline-block;line-height: 35rpx;padding:0 10rpx;margin-right: 10rpx;
}
.fw-store .button:nth-child(2){
  color: #CCAA77;
}
.fw-store{
  position: relative;
}
.st-ser .fw-title{
  height: 90rpx;
}
.fw-title-active{
  position: fixed;z-index:1000;
  width:100%;top: 0;
}
.st-ser .fw-title>view {
  width: 33%;font-size: 30rpx;font-weight: bold;padding-top: 45rpx;
}
.st-ser{
  position:fixed;width: 100%;z-index: 1000;background: #fff;padding-bottom: 80rpx;
  height: 100%;background: rgba(0, 0, 0, 0.5);top: 0;z-index: 100;padding-top:40rpx;
}
.fw-title>view.active view{
  border-bottom:4rpx solid #CCAA77;padding-bottom: 20rpx;
}
.st-ser-cont>view{
  display: inline-table;width: 45%;font-size:28rpx;
}
.st-ser-cont,.show-floor{
  padding-top: 30rpx;background: #fff;
  }
.show-floor{
  text-align: center;padding-bottom: 30rpx;color: #CCAA77
}
.st-ser-cont>view view{
  padding-left: 60rpx;height: 85rpx;line-height: 85rpx;
}
.st-ser-cont>view.ser-left{
  width: 55%;background: #F5F5F5;
}
.st-ser-cont>view.ser-left .active{
  background: #fff
}
.st-ser-cont>view.ser-right view:first-child{
  color: #CCAA77
}
.st-hide{
  position: fixed;height: 100%;width: 100%;background: rgba(0, 0, 0, 0.5);top: 0;z-index: 100;
}
.st-ser  .fw-title{
  height: 117rpx;
}
