    </view> -->
    <view class='gd_r-title'>
      <text>{{introduction}}</text>
    </view>
  </view>
  
  <view class='gd_dp'>
    <view class="weui_cells weui_cells1 gd_r_text">
        <text>优店介绍 </text>
    </view>
      <image src="http://pic.youlife.me/v1/tfs/T18RKTB4ET1RXrhCrK.jpg"></image>
