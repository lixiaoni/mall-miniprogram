.cart-list{
    position: relative;
    margin-left:265rpx;
    height: 190rpx;
    padding-top:35rpx;
    border-bottom: 2rpx solid #ECECEC;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: -235rpx;
    top: 85rpx;
    width: 45rpx;
    height: 45rpx;
}
