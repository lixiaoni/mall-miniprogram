
.fw-store{
  background: #fff;margin: 20rpx;position: relative;
  border-radius: 10rpx;padding:40rpx 50rpx;
}
.fw-store .fw-top{
  position: relative;font-size:36rpx;
  padding-left:90rpx;margin-top: 10rpx;
}
.fw-top view.fw-iew{
  font-size: 24rpx;color: #999
}
.fw-top view.fw-iew text{
  color: #CCAA77;margin-right: 20rpx;
}
.fw-top view.fw-iew1 text{
  background: #CCAA77;font-size: 24rpx;color: #fff;border-radius: 8rpx;
  padding:2rpx 10rpx 5rpx 10rpx;margin-left: 10rpx;
}
.fw-top view.fw-iew1 text:nth-child(2){
  background:#FF5C52;
}
.fw-store .fw-logo{
  height: 70rpx;width: 70rpx;position: absolute;left: 0
}
.fw-store .fw-logo{
  max-height: 70rpx;max-width: 70rpx;border-radius: 10rpx;
}
.lw-pop-cont>view .fw-logo{
  height: 70rpx;width: 70rpx;margin:0 auto;padding-bottom: 30rpx;
}
.lw-pop-cont>view .fw-logo image{
  max-height: 70rpx;max-width: 70rpx;border-radius: 10rpx;
}
.fw-store .fw-left{
  width:35rpx;height: 35rpx;float: right;margin-top: -60rpx;
}
.fw-cont{
  font-size: 26rpx;color: #999;
}
.fw-cont text{
  color: #333;
}
.fw-cont .fw-you{
  color: #CCAA77
}
.fw-cont>view{
  display: inline-block;width: 33.3%;margin-top:35rpx;text-align: center;
}
.fw-c-img{
  height: 200rpx;width: 200rpx;border-radius: 10rpx;margin-bottom: 40rpx;text-align: center;
}
.fw-c-img image{
  max-height: 200rpx;max-width: 200rpx
}
.fw-c-you{
  height: 35rpx;width: 35rpx;vertical-align: middle;margin-top: -2rpx;
}
.fw-cont>view view:nth-child(2),.fw-cont>view view:nth-child(3),.fw-cont>view view:nth-child(4){
  text-align: left;margin-left: 10rpx;
}
.fw-no{
  font-size: 48rpx;background: #fff;border-top: 2rpx solid #EFEFEF;padding-top: 50rpx;padding-left:50rpx;font-weight: bold;
  padding-bottom:25rpx;
}
.fw-no view:last-child{
  font-size: 24rpx;color: #666;font-weight: normal
}
.fw-no view:first-child{
  line-height: 40rpx;
}
.lw-pop{
  padding:35rpx 50rpx;font-size: 40rpx;background: #fff;
}
.lw-pop>view:first-child{
 font-weight: bold;
}
.lw-pop>view:first-child text{
   color: #CCAA77;font-size: 30rpx;float: right;font-weight: normal
}
.lw-pop>view:nth-child(2){
  font-size: 28rpx;
}
.miniProgram{
  bottom: 0;top: 0;
}
.lw-pop-cont>view{
  display: inline-block;width:30%;border-radius: 10rpx;background: #FAFAFA;font-weight: bold;height: 250rpx;position: relative;
  text-align: center;font-size: 28rpx;padding:35rpx 0;margin-top: 30rpx;margin-left: 10rpx;margin-right: 10rpx;
}
.lw-pop-cont>view view:nth-child(3){
  font-size: 24rpx;color:#999;font-weight: normal;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;padding:0 10rpx;
  -webkit-box-orient: vertical;
}
.lw-pop-cont>view view:nth-child(4){
  height:50rpx;width: 120rpx;background: #fff;color: #CCAA77;border-radius: 8rpx;margin:0 auto;margin-top: 20rpx;font-weight: normal
}
.lw-pop-cont>view image{
  height: 70rpx;width: 70rpx;border-radius: 8rpx;
}