<scroll-view scroll-y="true" style="height:100%;" bindscrolltolower="bindDownLoad">
  <view>
    <view class='fw-store' wx:for="{{dataList}}" wx:key="{{index}}">
    <navigator class='miniProgram' target="miniProgram" open-type="navigate" app-id="wx4f385374765e4cbb" path="pages/page/home/home?storeId={{item.storeId}}" extra-data="" version="trial"></navigator>
      <view class='fw-top'>
        <image class='fw-logo' src='{{item.store.logo}}'></image>
        <view class='fw-iew1'>{{item.store.name}}<text>新批零</text><text>友</text></view>
        <view class='fw-iew'><text>{{item.store.businessScope}}</text>{{item.store.address}}</view>
        <image class='fw-left' src='/image/mallr.png'></image>
      </view>
      <view class='fw-cont'>
        <view wx:for="{{item.goodsList}}" wx:key="{{index}}">
            <image class='fw-c-img' src='{{item.mainImgUrl}}' ></image>
            <view>零售 <text>￥{{item.sellPrice}}</text></view>
            <view>批发 <text class='fw-you'>￥{{item.wholesalePrice}}</text></view>
          </view>
      </view>
      <view> <view class='button'>购买过的店</view></view>
    </view>
  </view>
</scroll-view>
