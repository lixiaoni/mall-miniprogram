.swiper-tab{ 
  width: 100%; 
  margin-top: -9rpx;
  text-align: center; 
  background: #fff;
  line-height: 80rpx;} 
.swiper-tab-item{ 
  font-size: 28rpx; 
  display: inline-block; 
  width: 33.33%; 
  color: #666; 
} 
.swiper-tab-item image{
  width: 25rpx;
  vertical-align: middle;
  margin-left: 5rpx;
  height: 25rpx;
}
.on{ 
  color: #C79359; position: relative;
  } 
.swiper-box{ display: block; height: 100%; width: 100%; overflow: hidden; } 
.swiper-box view{ 
  text-align: center; 
} 
.on view{
  height: 2rpx;
  width: 80rpx;
  background:#C79359;
  left: 50%;
  margin:0 auto;
}
.goods-total{
  position: fixed;
  font-size: 20rpx;
  color: #666;
  padding-top: 5rpx;
  height: 85rpx;
  width: 90rpx;
  bottom: 30rpx;
  right: 30rpx;
  border-radius: 50%;
  border:2rpx solid #E7E7E7;
  box-shadow: 0 5rpx 10rpx #E7E7E7;
  background: #FDFDFD;
  text-align: center
}
.goods-total view{
  width: 65rpx;height: 2rpx;background: #E7E7E7;
  margin:0 auto;
}