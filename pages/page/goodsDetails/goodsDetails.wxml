            </view>
            <view class="ym_goodsstoreop">
                <navigator class="ui-link"><text class="ym_qzjoin_add1">+</text>关注</navigator>
                <navigator class="ui-link ui-link1">进店</navigator>
            </view>
        </view>
  <view class='gd_readCont'>
