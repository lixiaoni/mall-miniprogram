<view class="goods-ser">
    <input placeholder="输入商品名称" value="{{value}}"  bindblur="blurInputEvent" />
    <image class="home-ser"  src="/image/search.png"></image>
    <view class='home-img'>
      <navigator url="">
        筛选
      </navigator>
      <view hidden="{{}}" class='st-xiala alert-xiala'>
        <view class='arrow-up'></view>
        <view class="{{alertTab==0 ? 'active' : ''}}" data-current="0" bindtap="alertNav">全部商品</view>
        <view class="{{alertTab==1 ? 'active' : ''}}" data-current="1" bindtap="alertNav">引用商品</view>
        <view class="{{alertTab==2 ? 'active' : ''}}" data-current="2" bindtap="alertNav">自建商品</view>
    </view>
    </view>
</view>
<view class='st-title'>
  <view class="{{currentTab==0 ? 'active' : ''}}" data-current="0" bindtap="swichNav">出售中</view>
  <view class="{{currentTab==1 ? 'active' : ''}}" data-current="1" bindtap="swichNav">仓库中</view>
  <view class="{{currentTab==2 ? 'active' : ''}}" data-current="2" bindtap="swichNav">已售完</view>
  <view class="{{currentTab==3 ? 'active' : ''}}" data-current="3" bindtap="swichNav">本店分类<image  src="/image/xl.png"></image></view>
</view>
<view hidden="{{hidden}}" class='st-xiala'>
    <view>商品</view>
    <view>商品</view>
    <view>商品</view>
    <view>商品</view>
</view>
<view>
 <view class="record-box"  wx:for="{{detailList}}" wx:key="{{index}}" wx:for-item="record">
  <view class="record" style="left:{{record.offsetX}}px;left:{{leftVal}}px" data-index="{{index}}" bindtouchstart="recordStart" catchtouchmove="recordMove" bindtouchend="recordEnd">
    <view class="st-cont">
          <navigator><image class="cart-thumb" src="{{record.image}}"></image></navigator>
               <view  class='st-d-t'>{{record.title}}</view> 
                <view class='st-details'>
                  <view class='st-d-status'>
                    <view><text>批发:</text><text class='st-mom'>￥1400</text></view>
                    <view><text>零售:</text><text class='st-mom'>￥1400</text></view>
                    <view>供货：400</view><view>零售：400</view>
                    <view>库存：400 <image class='warn' src="/image/warn.png"></image></view><view>引用：400</view>
                  </view>
                   <view class='not'><view class='triangle_border_up'></view>该商品库存不足 请及时补货</view>
                  </view>
              </view>
              <view class='st-bottom'>
        <view>转发</view>
        <view>预览</view>
        <view>下架</view>
      </view>
            </view>
            <view class="delete-box" bindtap="deleteList" data-index="{{index}}">
            <view>删除</view>
            </view>
          </view>
      </view>


